{
  "canisters": {
    "droute": {
      "main": "backend/droute/main.mo",
      "type": "motoko"
    },
    "nisp": {
      "main": "backend/nisp/main.mo",
      "type": "motoko"
    },
    "test_runner": {
      "main": "backend/tests/test_runner.mo",
      "type": "motoko"
    },
    "test_runner_droute_utilities": {
      "main": "backend/tests/test_runner_droute_utilities.mo",
      "type": "motoko"
    },
    "test_publisher": {
      "main": "backend/tests/test_publisher.mo",
      "type": "motoko",
      "dependencies": [
        "droute"
      ]
    },
    "test_nisp": {
      "main": "backend/tests/test_nisp.mo",
      "type": "motoko",
      "dependencies": [
        "nisp"
      ]
    },
    "starter": {
      "main": "backend/starter/main.mo",
      "type": "motoko"
    },
    
    "assets": {
      "dependencies": [
        "starter"
      ],
      "frontend": {
        "entrypoint": "dist/index.html"
      },
      "source": [
        "dist/"
      ],
      "type": "assets"
    }
  },
  "defaults": {
    "build": {
      "args": "",
      "packtool": "vessel sources"
    }
  },
  "dfx": "0.10.0",
  "networks": {
    "local": {
      "bind": "127.0.0.1:8000",
      "type": "ephemeral"
    },
    "ic": {
      "providers": [
        "https://mainnet.dfinity.network"
      ],
      "type": "persistent"
    }
  },
  "version": 1
}